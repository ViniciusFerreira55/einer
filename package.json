{
  "name": "tcc",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "tests/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "mock": "turbo run mock",
    "generate": "turbo run generate",
    "db-push": "turbo run db-push",
    "docs": "turbo run docs",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.11",
    "babel-jest": "^29.3.1",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.3.1",
    "prettier": "latest",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "turbo": "latest",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.14.2"
}