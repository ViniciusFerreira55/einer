{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "AUTH_SERVICE_PORT",
    "AUTH_SERVER_REDIS_URL",
    "VITE_AUTH",
    "BORUM_SERVER_PORT",
    "VITE_BORUM",
    "PLANNER_SERVER_PORT",
    "VITE_PLANNER",
    "VSDIA_SERVER_PORT",
    "VITE_VSDIA",
    "S3_PORT"
  ],
  "globalDependencies": [
    ".env"
  ]
}
